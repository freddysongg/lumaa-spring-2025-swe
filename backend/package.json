{
  "name": "task-tracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Task Tracker application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev ts-node src/scripts/init-db.ts",
    "test": "jest",
    "format": "prettier --config .prettierrc --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --config .prettierrc --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint ."
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.20.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.5.1",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.24.1"
  },
  "_moduleAliases": {
    "@": "dist",
    "@config": "dist/config",
    "@controllers": "dist/controllers",
    "@middleware": "dist/middleware",
    "@routes": "dist/routes",
    "@lib": "dist/lib",
    "@types": "dist/types"
  }
}
